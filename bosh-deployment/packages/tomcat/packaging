set -e -x

wget http://javadl.oracle.com/webapps/download/AutoDL?BundleId=225343_090f390dda5b47b9b721c7dfaa008135 -O ${BOSH_INSTALL_TARGET}/jre-8u144-linux-i586.tar
tar xvf ${BOSH_INSTALL_TARGET}/jre-8u144-linux-i586.tar
${BOSH_INSTALL_TARGET}/jre1.8.0_144/bin/java -version

cp apache-tomcat-7.0.81-robocode.tar.gz ${BOSH_INSTALL_TARGET}
cd ${BOSH_INSTALL_TARGET}

tar xzvf apache-tomcat-7.0.81-robocode.tar.gz

